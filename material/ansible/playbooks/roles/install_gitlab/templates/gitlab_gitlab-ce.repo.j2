[gitlab_gitlab-ce]
name=gitlab_gitlab-ce
baseurl={{ gitlab_repo_info.baseurl }}
repo_gpgcheck=1
gpgcheck=1
enabled=1
gpgkey={{ gitlab_repo_info.gpgkey.key }}
       {{ gitlab_repo_info.gpgkey.pub_key }}
sslverify=1
sslcacert=/etc/pki/tls/certs/ca-bundle.crt
metadata_expire=300

[gitlab_gitlab-ce-source]
name=gitlab_gitlab-ce-source
baseurl={{ gitlab_repo_info_source.baseurl }}
repo_gpgcheck=1
gpgcheck=1
enabled=1
gpgkey={{ gitlab_repo_info_source.gpgkey.key }}
       {{ gitlab_repo_info_source.gpgkey.pub_key }}
sslverify=1
sslcacert=/etc/pki/tls/certs/ca-bundle.crt
metadata_expire=300

